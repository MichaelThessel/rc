[user]
	email = foo@example.com
	name = Michael Thessel
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	d = difftool
	co = checkout
	s = status
	ec = config --global -e
	up = !git pull --rebase $@ && git submodule update --init --recursive
	cob = checkout -b
	cm = !git add -A && git commit -m
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[core]
	excludesfile = /home/michael/.gitignore
[credential]
	helper = cache --timeout=86400
[http]
	sslVerify = false
[pull]
	rebase = true
